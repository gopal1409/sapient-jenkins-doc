pipeline {
    agent any 
    // agent is where my pipeline will be eexecuted
    tools {
        //install the maven version configured as m2 and add it to the path
        maven "m3"
    }
    stages {
        stage('pull from scm') {
            git credentialsId: 'gitlab-token', url: 'https://github.com/gopal1409/springchat1.git'
        }
    }
}
